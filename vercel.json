{
    "version": 2,
    "builds": [
      {
        "src": "apis/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/index.js",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/apis/$1.js"
      },
      {
        "src": "/(.*)",
        "dest": "/src/index.js"
      }
    ]
  }
  